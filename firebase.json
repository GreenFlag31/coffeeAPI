{
  "hosting": {
    "public": "public",
    "rewrites": [{ "source": "**", "function": "api" }],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**", "**/middleware/**", "**/models/**"]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": ["node_modules", ".git", "firebase-debug.log", "firebase-debug.*.log"]
    }
  ]
}
